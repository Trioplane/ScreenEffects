{
    "name": "ScreenEffects",
    "description": "A small flexible library for showing animated screen overlays to the user.",
    "version": "v1.1.1",
    "author": "Trplnr",
    "require": ["bolt"],
    "data_pack": {
      "load": ["./src"],
      "pack_format": 71
    },
    "resource_pack": {
      "load": ["./src"],
      "pack_format": 55
    },
    "pipeline": ["mecha"],
    "output": "./build",
    "meta": {
      "bolt": {"entrypoint": "scrfx:*"}
    }
}